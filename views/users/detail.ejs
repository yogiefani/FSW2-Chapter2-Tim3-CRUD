<div class="flex d-flex justify-content-center">
    <div class="card" style="width: 18rem;">
        <img src="<%= User.photoProfile %>" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title"><%= User.name %></h5>
            <p class="card-text"><%= User.email %></p>
            <p class="card-text"><%= User.phoneNumber %></p>
            <p class="card-text"><%= User.role.name ? User.role.name : 'No Role Assigned' %></p>


        </div>
    </div>
</div>